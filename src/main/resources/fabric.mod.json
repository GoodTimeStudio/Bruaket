{
	"schemaVersion": 1,
	"id": "bruaket",
	"version": "${version}",
	"name": "bruaket",
	"description": "This is an example description! Tell everyone what your mod is about!",
	"authors": [
		"GoodtimeStudio",
		"Seedking"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/bruaket/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"value": "com.github.goodtimestudio.Bruaket",
				"adapter": "kotlin"
			}
		],
		"client": [
			{
				"value": "com.github.goodtimestudio.BruaketClient",
				"adapter": "kotlin"
			}
		],
		"fabric-datagen": [
			{
				"value": "com.github.goodtimestudio.BruaketDataGenerator",
				"adapter": "kotlin"
			}
		]
	},
	"mixins": [
		"bruaket.mixins.json",
		{
			"config": "bruaket.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.14.22",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*",
		"fabric-language-kotlin": ">=1.9.0"
	},
	"suggests": {
		"another-mod": "*"
	}
}